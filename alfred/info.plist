            <key>type</key>
            <string>alfred.workflow.action.script</string>
            <key>uid</key>
            <string>action-script</string>
            <key>config</key>
            <dict>
                <key>script</key>
                <string>export PATH="/usr/local/bin:/opt/homebrew/bin:$PATH"
focusbm restore "{query}"</string>
                <key>type</key>
                <integer>0</integer>
            </dict>
        </dict>
    </array>
    <key>connections</key>
    <dict>
        <key>input-scriptfilter</key>
        <array>
            <dict>
                <key>destinationuid</key>
                <string>action-script</string>
            </dict>
        </array>
    </dict>
    <key>uidata</key>
    <dict>
        <key>input-scriptfilter</key>
        <dict>
            <key>xpos</key>
            <integer>100</integer>
            <key>ypos</key>
            <integer>100</integer>
        </dict>
        <key>action-script</key>
        <dict>
            <key>xpos</key>
            <integer>300</integer>
            <key>ypos</key>
            <integer>100</integer>
        </dict>
    </dict>
