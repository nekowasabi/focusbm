{
  "name": "focusbm",
  "title": "focusbm",
  "description": "Focus bookmark switcher for macOS",
  "icon": "extension-icon.png",
  "author": "focusbm",
  "license": "MIT",
  "commands": [
    {
      "name": "search-bookmarks",
      "title": "Search Bookmarks",
      "description": "Search and restore focus bookmarks",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "cliPath",
      "title": "CLI Path",
      "description": "Path to focusbm binary",
      "type": "textfield",
      "required": false,
      "default": "/usr/local/bin/focusbm"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.60.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
